editsList = [
[name:'a', FlagMnemonic:'REB', Action:'deny'],
[name:'b', FlagMnemonic:'TRA', Action:'review'],
[name:'c', FlagMnemonic:'UNB', Action:'deny'],
[name:'d', FlagMnemonic:'REB', Action:'deny']
]

param = [a:'1', b:'2', c:'1']  

def thisValue2 = appliedEditsMap('name', 'a') ?: "default"
println thisValue2

def appliedEditsMap = {property,idValue->
     return editsList.find{it[property] == idValue}
}



param.each { first ->
    println("$first");
    def p = appliedEditsMap('name', first.key) ?: "default"
    println p.find{ it.key == "FlagMnemonic" }?.value

}

